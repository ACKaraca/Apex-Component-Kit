{
  "name": "@ack/cicd",
  "version": "0.10.0",
  "description": "ACK Framework CI/CD Pipeline - GitHub Actions, automated testing, deployment",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",

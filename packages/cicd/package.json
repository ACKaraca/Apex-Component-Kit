  "license": "MIT",
  "dependencies": {
    "@ack/runtime": "workspace:*",
    "@ack/kit": "workspace:*",
    "@ack/compiler": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  }
}

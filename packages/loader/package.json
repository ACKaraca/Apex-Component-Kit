{
  "name": "@ack/loader",
  "version": "0.6.0",
  "description": "ACK Lazy Loading & Code Splitting - Dynamic imports, chunk management, prefetching",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@ack/runtime": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "keywords": [
    "ack",
    "loader",
    "lazy-loading",
    "code-splitting",
    "chunk-management",
    "prefetching",
    "dynamic-import"
  ]
}

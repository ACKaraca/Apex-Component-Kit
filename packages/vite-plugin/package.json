{
  "name": "@ack/vite-plugin",
  "version": "0.0.1",
  "description": "Vite plugin for ACK framework - integrates .ack components with Vite dev server",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "lint": "tsc --noEmit"
  },
  "peerDependencies": {
    "vite": "^5.0.0",
    "@ack/compiler": "^0.0.1"
  },
  "dependencies": {
    "@ack/compiler": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4",
    "vite": "^5.0.5"
  }
}

{
  "name": "@ack/store",
  "version": "0.4.0",
  "description": "ACK State Management - Reactive global state with actions, mutations, and persistence",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"

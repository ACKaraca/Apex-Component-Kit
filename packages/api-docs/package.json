{
  "name": "@ack/api-docs",
  "version": "0.11.0",
  "description": "ACK Framework API Documentation - Swagger/OpenAPI integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "echo 'No linting configured'",
    "generate": "node dist/scripts/generate-docs.js",
    "serve": "node dist/server.js",
    "dev:server": "ts-node src/server.ts"
  },
  "keywords": [
    "ack",
    "api",
    "documentation",
    "swagger",
    "openapi"
  ],
  "author": "ACK Framework Team",
  "license": "MIT",
  "dependencies": {
    "@ack/runtime": "workspace:*",
    "@ack/api": "workspace:*",
    "express": "^4.18.2",
    "swagger-ui-express": "^4.6.3",
    "swagger-jsdoc": "^6.2.8",
    "cors": "^2.8.5",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "vitest": "^1.0.4",
    "@types/express": "^4.17.17",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/cors": "^2.8.13",
    "@types/yaml": "^2.0.0",
    "ts-node": "^10.9.1"
  }
}





{
  "name": "@ack/api",
  "version": "0.5.0",
  "description": "ACK API Integration - HTTP client, GraphQL, caching, and real-time data fetching",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest --run",
    "test:watch": "vitest",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@ack/store": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  },
  "keywords": [
    "ack",
    "api",
    "http-client",
    "graphql",
    "data-fetching",
    "caching",
    "real-time"
  ]
}

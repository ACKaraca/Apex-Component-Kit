    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@ack/kit": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4"
  }

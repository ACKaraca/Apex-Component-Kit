{
  "name": "@ack/cli",
  "version": "0.0.1",
  "description": "ACK CLI - Project scaffolding and management",
  "license": "MIT",
  "type": "module",
  "bin": {
    "create-ack-app": "./dist/create.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
